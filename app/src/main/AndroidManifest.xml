<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ryanbrooks.expandablerecyclerviewsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name="ClientApp"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <meta-data android:name="DATABASE" android:value="MYDB" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />`
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.ryanbrooks.expandablerecyclerviewsample" />


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".linear.vertical.VerticalLinearRecyclerViewSampleActivity"
            android:label="@string/activity_vertical_linear_sample_title">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>
